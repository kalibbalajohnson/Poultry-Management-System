    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "mocha": "^10.7.3",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
